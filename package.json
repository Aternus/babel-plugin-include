{
  "name": "@aternus/babel-plugin-include",
  "version": "1.0.20",
  "description": "Adds the ability to use `include` as part of the JavaScript syntax. Includes external JavaScript code inline.",
  "keywords": [
    "babel",
    "plugin",
    "javascript",
    "include",
    "external",
    "file",
    "inline"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "npx babel -d ./dist ./src",
    "prepublishOnly": "npx babel -d ./dist ./src --minified --no-comments"
  },
  "author": "Kiril Reznik",
  "license": "MIT",
  "homepage": "https://github.com/Aternus/babel-plugin-include",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aternus/babel-plugin-include.git"
  },
  "bugs": {
    "url": "https://github.com/Aternus/babel-plugin-include/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@types/node": "^16.11.26",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/parser": "^7.12.16"
  }
}
