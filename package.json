{
  "name": "@aternus/babel-plugin-include",
  "version": "1.0.26",
  "description": "Adds the ability to use `include` as part of the JavaScript syntax. Includes external JavaScript code inline.",
  "keywords": [
    "babel",
    "plugin",
    "javascript",
    "include",
    "external",
    "file",
    "inline"
  ],
  "type": "commonjs",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build:dist": "npx tsc --build",
    "build:dist:dev": "npx tsc -p tsconfig.dev.json --build",
    "build:watch": "npx tsc --build --watch",
    "build": "npm-run-all clean build:dist",
    "watch": "npm-run-all clean build:watch",
    "lint:prettier": "npx prettier --write .",
    "lint:eslint": "npx eslint --fix .",
    "lint": "npm-run-all lint:prettier lint:eslint",
    "pretest": "npm-run-all lint build",
    "test": "npx babel --plugins ./dist/src/index.js test/main.js",
    "predev": "npm-run-all build:dist:dev",
    "dev": "npx babel --plugins ./dist/src/index.js test/main.js",
    "prepublishOnly": "npx babel -d ./dist ./src --minified --no-comments"
  },
  "author": "Kiril Reznik",
  "license": "MIT",
  "homepage": "https://github.com/Aternus/babel-plugin-include",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aternus/babel-plugin-include.git"
  },
  "bugs": {
    "url": "https://github.com/Aternus/babel-plugin-include/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@types/babel__core": "^7.20.1",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "typescript": "~5.0"
  },
  "dependencies": {
    "@babel/core": "^7.22.5"
  },
  "engines": {
    "node": ">=14"
  }
}
